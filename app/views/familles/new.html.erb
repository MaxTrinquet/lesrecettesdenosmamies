  <h1><strong>Votre Famille </strong></h1></div>

  <div class="container">
    <%= simple_form_for @famille do |f| %>
      <%= f.error_notification %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, class: "btn btn-primary font-weight-bold" %>
    <% end %>
  </div>
